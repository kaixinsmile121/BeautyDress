<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="40dp"
    android:gravity="center_vertical">
    <ImageView
        android:id="@+id/me_lv_01_iv_01"
        android:layout_width="27dp"
        android:layout_height="37dp"
        android:paddingLeft="8dp"
        android:src="@mipmap/kimagel_order_all"
        />
    <TextView
        android:id="@+id/me_lv_01_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="全部订单"
        android:textSize="12sp"
        android:layout_marginTop="7dp"
        android:layout_marginLeft="10dp"
        android:layout_toRightOf="@id/me_lv_01_iv_01"/>
    <ImageView
        android:id="@+id/me_lv_01_iv_02"
        android:layout_alignParentRight="true"
        android:layout_marginRight="20dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/yuike_settings_indicator_un"
        android:layout_marginTop="5dp"/>
</RelativeLayout>
